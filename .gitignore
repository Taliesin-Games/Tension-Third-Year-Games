HeartOfRuin/*.booproj
HeartOfRuin/*.svd
HeartOfRuin/*.pdb
HeartOfRuin/*.mdb
HeartOfRuin/*.opendb
HeartOfRuin/*.VC.db

# Unity3D generated meta files
HeartOfRuin/*.pidb.meta
HeartOfRuin/*.pdb.meta
HeartOfRuin/*.mdb.meta

# Unity3D generated file on crash reports
HeartOfRuin/sysinfo.txt

# Mono auto generated files
HeartOfRuin/mono_crash.*

# Builds
HeartOfRuin/*.apk
HeartOfRuin/*.aab
HeartOfRuin/*.unitypackage
HeartOfRuin/*.unitypackage.meta
HeartOfRuin/*.app

# Crashlytics generated file
HeartOfRuin/crashlytics-build.properties

# TestRunner generated files
HeartOfRuin/InitTestScene*.unity*

# Addressables default ignores
HeartOfRuin/ServerData
HeartOfRuin/[Aa]ssets/StreamingAssets/aa*
HeartOfRuin/[Aa]ssets/AddressableAssetsData/link.xml*
HeartOfRuin/[Aa]ssets/Addressables_Temp*
HeartOfRuin/[Aa]ssets/AddressableAssetsData/*/*.bin*

# Visual Scripting auto-generated files
HeartOfRuin/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db
HeartOfRuin/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db.meta
HeartOfRuin/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers
HeartOfRuin/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers.meta

# Auto-generated scenes by play mode tests
HeartOfRuin/[Aa]ssets/[Ii]nit[Tt]est[Ss]cene*.unity*


