UnityProject/*.mdb.meta

# Unity3D generated file on crash reports
UnityProject/sysinfo.txt

# Mono auto generated files
UnityProject/mono_crash.*

# Builds
UnityProject/*.apk
UnityProject/*.aab
UnityProject/*.unitypackage
UnityProject/*.unitypackage.meta
UnityProject/*.app

# Crashlytics generated file
UnityProject/crashlytics-build.properties

# TestRunner generated files
UnityProject/InitTestScene*.unity*

# Addressables default ignores
UnityProject/ServerData
UnityProject/[Aa]ssets/StreamingAssets/aa*
